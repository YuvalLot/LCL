
set Init
    case ?x then hInit(?x);

set Auto
    case ?x then hAuto(?x);

set Deref
    case ?x then hDel(?x);

set Ref
    case ?x, ?y then hRef(?x, ?y) $ hAssign(?x, ?y);

set GetRef
    case ?x, ?y then hRef(?x, ?y);

set SetRef
    case ?x, ?y then hAssign(?x, ?y);

set Scope
    case ?x then hScope(?x);

set Rcopy
    case ?x, ?y then hCopy(?x, ?y);

set Flush
    case then hFlush();

set PrintRef
    case ?ref then GetRef(?ref, ?x) & Print(?x);